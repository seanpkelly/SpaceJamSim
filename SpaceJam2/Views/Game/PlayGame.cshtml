@model List<double>
    <style>
        body {
            background-image: url('/Images/LooneyCircle.png');
            background-repeat: no-repeat;
            background-attachment: fixed;
            background-size: 100% 100%;
        }
    </style>

<h1>That's all Folks!</h1>
@if (Model[0] > Model[1])
{
    <h1>You Win! @Model[0] - @Model[1]</h1>
}
else if (Model[0] < Model[1])
{
<h1>Oh No, You Lose! @Model[0] - @Model[1] </h1>
}
else
{
<h1>You are so evenly matched, you tied! @Model[0] - @Model[1]</h1>
}
<a href="/Home/ViewTeams">Click here to go back to teams</a>
